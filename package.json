{
  "name": "efis-editor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "NG_HMR_TEMPLATES=1 ng serve",
    "start:prod": "ng build --configuration production && npx http-server -p 4200 -c-1 dist/efis-editor/browser",
    "start:prod-ssl": "ng build --configuration production && npx http-server-ssl -S -p 4200 -c-1 dist/efis-editor/browser",
    "start:ssr": "ng build && node dist/efis-editor/server/server.mjs",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /efis-editor/",
    "genproto": "npx protoc --proto_path=src/model/proto --plugin=protoc-gen-ts=node_modules/@protobuf-ts/plugin/bin/protoc-gen-ts --ts_out=gen/ts src/model/proto/*.proto",
    "genkeys": "[ -f src/environments/dev-keys.ts ] || echo 'export const GDRIVE_CLIENT_ID = '\\''YOUR_ID'\\'';' > src/environments/dev-keys.ts",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "format": "prettier --check \"{./*,src/**/*}.{js,json,ts,html,css,scss}\" \"./public/site.webmanifest\"",
    "format:fix": "npm run format -- --write",
    "spell": "codespell src/",
    "spell:fix": "codespell -w -i 3 src/",
    "prepush": "npm clean-install && npm run genproto && npm run lint && npm run format && npm run spell && npm run test:coverage && npx -y @bugron/validate-dependabot-yaml@latest",
    "prepush:fix": "npm run format:fix && npm run spell:fix",
    "push": "jj bookmark move main --to @- && jj git push -b main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/material": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/platform-server": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/service-worker": "^21.1.3",
    "@angular/ssr": "^21.1.3",
    "@ngneat/hotkeys": "^4.1.0",
    "@ngneat/until-destroy": "^10.0.0",
    "buffer": "^6.0.3",
    "buffer-crc32": "^1.0.0",
    "core-js": "^3.48.0",
    "escape-string-regexp": "^5.0.0",
    "express": "^5.2.1",
    "fast-deep-equal": "^3.1.3",
    "file-saver": "^2.0.5",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "mat-icon-button-sizes": "^1.0.6",
    "nanotar": "^0.2.0",
    "ngx-file-drop": "^16.0.0",
    "ngx-spinner": "^21.0.0",
    "rxjs": "~7.8.2",
    "scroll-into-view-if-needed": "^3.1.0",
    "svg2pdf.js": "^2.7.0",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-eslint/schematics": "^21.2.0",
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@eslint/css": "^0.14.1",
    "@protobuf-ts/plugin": "^2.11.1",
    "@testing-library/angular": "^19.0.0",
    "@testing-library/jasmine-dom": "^1.3.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.6",
    "@types/file-saver": "^2.0.7",
    "@types/gapi.client.drive-v3": "^0.0.5",
    "@types/google.accounts": "^0.0.18",
    "@types/jasmine": "~6.0.0",
    "@types/node": "^25.2.1",
    "@types/testing-library__jasmine-dom": "^1.3.4",
    "angular-eslint": "21.2.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-depend": "^1.4.0",
    "eslint-plugin-jasmine": "^4.2.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-rxjs-x": "^0.9.1",
    "eslint-plugin-testing-library": "^7.15.4",
    "jasmine-core": "~6.0.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.2.0",
    "karma-jasmine-order-reporter": "^1.2.0",
    "karma-spec-reporter": "^0.0.36",
    "pdfjs-dist": "^5.4.624",
    "prettier": "^3.8.1",
    "prettier-eslint": "^16.4.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.54.0"
  }
}
