{
  "protoc": {
    "path": "${workspaceRoot}/src/model/proto/protoc.sh",
    "compile_on_save": true,
    "compile_all_path": "${workspaceRoot}/src/model/proto",
    "use_absolute_path": true,
    "options": [
      "--proto_path=${workspaceRoot}/src/model/proto",
      "--plugin=protoc-gen-ts=${workspaceRoot}/node_modules/@protobuf-ts/plugin/bin/protoc-gen-ts",
      "--ts_out=${workspaceRoot}/gen/ts"
    ]
  },
  "formatFiles.excludePattern": "src/model/formats/test-foreflight.fmd,src/model/formats/test-garmin-pilot.gplt,src/model/formats/test.json,src/model/formats/test.ace"
}
