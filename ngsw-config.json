{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.svg",
          "/index.csr.html",
          "/index.html",
          "/checklists/index.html",
          "/site.webmanifest",
          "/*.css",
          "/*.js",
          "/**/*.svg",
          "/**/*.png",
          "/**/*.ico",
          "/**/*.ttf",
          "!/**/screenshot*.png"
        ]
      }
    },
    {
      "name": "lazy-app",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": ["https://fonts.googleapis.com/**", "https://fonts.gstatic.com/**", "https://ssl.gstatic.com/**"]
      }
    },
    {
      "name": "lazy-assets",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": ["/**/screenshot*.png"]
      }
    }
  ]
}
